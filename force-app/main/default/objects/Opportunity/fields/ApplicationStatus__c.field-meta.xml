<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ApplicationStatus__c</fullName>
    <description>This represents Membership application status and is displayed in membership portal. When New Business Membership Opportunity is created  
Default status = Application Received
When Docusign is initiated set status= Waiting for Customer Signature
When Docusign is signed by customer and waiting for LF signature status=Waiting for Linux Foundation Signature
When Docusign is complete and opportunity is still open status = Waiting to Close by PM
When Docusign is voided status= Application Received
When opportunity is closed won status= Complete
When opportunity is closed lost status= Cancelled</description>
    <externalId>false</externalId>
    <inlineHelpText>This status is displayed in membership portal. Reach out to platform service for any changes</inlineHelpText>
    <label>Application Status</label>
    <required>false</required>
    <trackFeedHistory>false</trackFeedHistory>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Picklist</type>
    <valueSet>
        <restricted>true</restricted>
        <valueSetDefinition>
            <sorted>false</sorted>
            <value>
                <fullName>Application Received</fullName>
                <default>true</default>
                <label>Application Received</label>
            </value>
            <value>
                <fullName>Waiting for Customer Signature</fullName>
                <default>false</default>
                <label>Waiting for Customer Signature</label>
            </value>
            <value>
                <fullName>Waiting for Linux Foundation Signature</fullName>
                <default>false</default>
                <label>Waiting for Linux Foundation Signature</label>
            </value>
            <value>
                <fullName>Waiting to Close by PM</fullName>
                <default>false</default>
                <label>Waiting to Close by PM</label>
            </value>
            <value>
                <fullName>Complete</fullName>
                <default>false</default>
                <label>Complete</label>
            </value>
            <value>
                <fullName>Cancelled</fullName>
                <default>false</default>
                <label>Cancelled</label>
            </value>
            <value>
                <fullName>Application in Review</fullName>
                <default>false</default>
                <isActive>false</isActive>
                <label>Application in Review</label>
            </value>
        </valueSetDefinition>
    </valueSet>
</CustomField>
